{
  "name": "prong-docs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "old-build": "tsc && vite build",
    "build": "vite build && npm run prep-data-prod && npm run cp-quiets && npm run cp-docs",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "cp-quiets": "cp ./src/examples/Quiet* ./public/",
    "cp-docs": "cp ../../README.md ./public/",
    "prep-data-prod": "mkdir -p dist/data && rm -rf dist/data && mkdir dist/data && cp ../../node_modules/vega-datasets/data/* ./dist/data/",
    "prep-data": "rm -rf public/data && mkdir public/data && cp ../../node_modules/vega-datasets/data/* ./public/data/"
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.1.4",
    "d3-array": "^3.2.1",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.0.0",
    "d3-shape": "^3.2.0",
    "estree-walker": "^3.0.3",
    "lodash.merge": "^4.6.2",
    "prong-editor": "*",
    "react": "^17.0.2",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^17.0.2",
    "react-markdown": "^7.1.0",
    "react-router": "^6.4.2",
    "react-router-dom": "^6.4.2",
    "react-syntax-highlighter": "^15.5.0",
    "react-vega": "^7.6.0",
    "seedrandom": "^3.0.5",
    "vega": "^5.22.1",
    "vega-datasets": "^2.7.0",
    "vega-embed": "^6.21.3",
    "vega-expression": "^5.0.0",
    "vega-lite": "^5.6.1"
  },
  "devDependencies": {
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/d3": "^7.4.0",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.11",
    "@types/react-router": "^5.1.20",
    "@types/react-syntax-highlighter": "^15.5.7",
    "@types/seedrandom": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^8.44.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "lerna": "^6.5.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.0"
  },
  "resolutions": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.14"
  },
  "workspaces": [
    "packages/*"
  ]
}